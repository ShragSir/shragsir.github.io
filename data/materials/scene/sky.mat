material base_sky
{
    pass
    {
        fog_override true
        lighting off
        ambient 1 1 1
        diffuse 0 0 0 1
        emissive 0 0 0
        texture_unit skyTex
        {
            direct_texture $texture
            create_in_ffp true
            tex_address_mode mirror
        }
    }
}

material sky/dust
{
    parent base_sky
    texture sky_dust.jpg
}

