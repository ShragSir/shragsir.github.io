
///  next check marker
material checkpoint
{
	parent base
	diffuseMap checkpoint.png
	ambient 0.1 0.1 0.1
	diffuse 0.1 0.1 0.1

	depth_write off
	scene_blend alpha_blend
	receives_shadows false

	//specular_alpha true
	cull_hardware none
	twoside_diffuse true
}

//  normal checkpoint, not finish
material checkpoint_normal
{
	parent checkpoint
	specular 0.2 0.4 0.2  32 //x4
	emissive 0.1 11.0 0.1
}
//  finish, does not end race
material checkpoint_lap
{
	parent checkpoint
	specular 0.3 0.4 0.4  32
	emissive 0.6 11.0 11.0
}
//  finish, last lap begins after passing it
material checkpoint_lastlap
{
	parent checkpoint
	specular 0.4 0.4 0.2  32
	emissive 11.0 11.0 0.1
}
//  the very last checkpoint, ends race after passing
material checkpoint_finish
{
	parent checkpoint
	specular 0.4 0.2 0.2  32
	emissive 11.0 0.7 0.1
}


//  bridges  base  -------------
material road
{
	parent base
	scene_blend alpha_blend
	alphaMap white.png
	depth_bias 3
	road_blend true

	specMap roadAsphalt_spec.jpg
	specMap_rgb true
}
//  on terrain  base  -------------
material road_terrain
{
	parent base
	depth_write off
	scene_blend alpha_blend
	alphaMap roadAlpha.png
	depth_bias 3
	terrain_light_map true
	road_blend true
}


//  trail ghost  ~~--~-~--
material trailRoad
{
	parent base
	scene_blend alpha_blend
	vertex_colour true
	road_blend true
	alphaMap white.png

	depth_bias 3
	depth_check off
	//depth_write off
	cull_hardware none
	fog_override true
	twoside_diffuse true
	receives_shadows false

	diffuseMap white.png
	ambient 1 1 1
	diffuse 0 0 0
	specular 0 0 1 32
}


