    {
        red   -0.02
        green -0.02
        blue  -0.02
        alpha 0.02
    }
    affector Scaler
    {
   	    rate 11.30
    }
	affector Rotator
	{
		rotation_range_start 0
		rotation_range_end 220
		rotation_speed_range_start -20
		rotation_speed_range_end 20
	}
	affector DirectionRandomiser
	{
		randomness	22
	}
}

