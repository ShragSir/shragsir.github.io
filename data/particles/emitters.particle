particle_system Cloud1
{
	material			Cloud1
	quota				1000
	particle_width		122
	particle_height		122
	cull_each			false
	billboard_type		point
	sorted				true

	emitter Cylinder
	{
		angle	0
		position	0 0 0
		direction	0 1 0
		width	232
		height	232
		depth	122  //h

		colour				0.9 0.9 0.9  0.2
		colour_range_start	0.9 0.9 0.9  0.1
		colour_range_end	0.9 0.9 0.9  0.3

		emission_rate	200
		velocity_min	0.1
		velocity_max	0.3
		time_to_live_min	2.3
		time_to_live_max	3.5
	}

